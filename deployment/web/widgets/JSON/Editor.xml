<widget id="JSON.widget.Editor" needsEntityContext="true" offlineCapable="true" xmlns="http://www.mendix.com/widget/1.0/">
    <name>JSON Editor</name>
    <description>JSON Editor/Viewer for Mendix</description>
    <icon></icon>
    <properties>
        <!-- DATA SOURCE -->
        <property key="jsonStringAttr" type="attribute" isPath="optional" pathType="reference" allowNonPersistableEntities="true">
            <caption>JSON String attribute</caption>
            <category>Data Source</category>
            <description>String that represent a JSON</description>
            <attributeTypes>
                <attributeType name="String" />
            </attributeTypes>
        </property>
        <property key="editable" type="boolean" defaultValue="true">
            <caption>Editable</caption>
            <category>Data Source</category>
            <description>Is the JSON object editable? This can be set to 'false' to act as a Viewer. If you set this to 'true', it will still respect the limitations within the page (using it in a non-editable Data View, a non-editable attribute, etcetera)</description>
        </property>
        <!-- Disabled until I can figure out how to do proper validations -->
        <!-- <property key="schemaStringAttr" type="attribute" required="false" isPath="optional" pathType="reference">
            <caption>JSON Schema attribute</caption>
            <category>Data Source</category>
            <description>String that represent a JSON Schema. This is used to validate against</description>
            <attributeTypes>
                <attributeType name="String" />
            </attributeTypes>
        </property> -->
        <!-- EVENTS -->
        <property key="onChangeMf" type="microflow" required="false">
            <caption>On Change microflow</caption>
            <category>Events</category>
            <description>Microflow to be called when there is a change. This will only be triggered if the JSON is valid.</description>
            <returnType type="Void" />
        </property>
        <!-- APPEARANCE -->
        <property key="height" type="integer" required="true" defaultValue="400">
            <caption>Height (px)</caption>
            <category>Appearance</category>
            <description>Height of the Editor. This is needed, as it is not auto growing</description>
        </property>
        <property key="showNavigationBar" type="boolean" defaultValue="true">
            <caption>Navigation bar</caption>
            <category>Appearance</category>
            <description>Show/hide navigationbar.</description>
        </property>
        <property key="showStatusBar" type="boolean" defaultValue="true">
            <caption>Status bar</caption>
            <category>Appearance</category>
            <description>Show/hide statusbar.</description>
        </property>
    </properties>
</widget>
